{
  "name": "point-of-view",
  "version": "5.1.0",
  "description": "Template plugin for Fastify",
  "main": "index.js",
  "types": "index.d.ts",
  "scripts": {
    "example:async": "node examples/example-async.js",
    "example:basic": "node examples/example-basic-ejs.js",
    "example:with-options": "node examples/example-ejs-with-some-options.js",
    "example:typescript": "npx ts-node examples/typescript-example.ts",
    "lint": "standard",
    "lint:fix": "standard --fix",
    "test": "npm run unit && npm run test:types",
    "test:dev": "npm run lint && npm run unit && npm run test:types",
    "test:with-snapshot": "cross-env TAP_SNAPSHOT=1 tap test/ejs-with-snapshot.test.js",
    "test:types": "tsd",
    "unit": "tap -J \"test/*.test.js\"",
    "unit:report": "npm run unit -- --coverage-report=html",
    "unit:verbose": "npm run unit -- -Rspec"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/fastify/point-of-view.git"
  },
  "keywords": [
    "fastify",
    "template",
    "view",
    "speed",
    "art-template",
    "dot",
    "ejs",
    "eta",
    "handlebars",
    "liquidjs",
    "mustache",
    "nunjucks",
    "pug",
    "twig"
  ],
  "author": "Tomas Della Vedova - @delvedor (http://delved.org)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/fastify/point-of-view/issues"
  },
  "homepage": "https://github.com/fastify/point-of-view#readme",
  "dependencies": {
    "fastify-plugin": "^3.0.1",
    "hashlru": "^2.3.0"
  },
  "devDependencies": {
    "@types/node": "^17.0.17",
    "art-template": "^4.13.2",
    "cross-env": "^7.0.3",
    "dot": "^1.1.3",
    "ejs": "^3.1.6",
    "eta": "^1.12.3",
    "express": "^4.17.2",
    "fastify": "^3.27.1",
    "handlebars": "^4.7.7",
    "html-minifier": "^4.0.0",
    "html-minify-stream": "^2.0.0",
    "liquidjs": "^9.34.0",
    "mustache": "^4.2.0",
    "nunjucks": "^3.2.3",
    "pre-commit": "^1.2.2",
    "proxyquire": "^2.1.3",
    "pug": "^3.0.2",
    "simple-get": "^4.0.1",
    "standard": "^16.0.4",
    "tap": "^15.1.6",
    "tsd": "^0.19.1",
    "twig": "^1.15.4",
    "typescript": "^4.5.5"
  },
  "tsd": {
    "directory": "test/types"
  }
}